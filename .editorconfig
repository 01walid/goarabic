# For more documentation, see: editorconfig.org
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.go]
indent_style = tab

[*.html]
indent_style = tab
indent_size = 2

[*.json]
indent_style = space
indent_size = 4

